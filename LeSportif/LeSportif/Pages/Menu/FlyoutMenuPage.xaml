<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LeSportif.Pages.Menu.FlyoutMenuPage"
             xmlns:pages="clr-namespace:LeSportif.Pages.Menu"
            NavigationPage.HasNavigationBar="False">
  <FlyoutPage.Flyout>
    <pages:FlyoutMenuPageFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:FlyoutMenuPageDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>